Arithmetic {
    Sum = NonemptyListOf<Sub, "+">
    Sub = NonemptyListOf<Mul, "-">
    Mul = NonemptyListOf<Div, "*">
    Div = NonemptyListOf<Primary, "/">
    Primary =
    | "(" Sum ")" -- parenthesis
    | "-" Sum -- unaryMin
    | number
    number =
    | digit+ --number
    | letter+ --variable
}